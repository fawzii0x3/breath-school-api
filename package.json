{
  "name": "rossi",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start-dev": "cross-env NODE_ENV=development node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon dist/index.js",
    "build": "npm run clean && tsc && npm run copy-assets",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "copy-assets": "cp -r src/configs dist/ && cp -r src/controllers dist/ && cp -r src/data dist/ && cp -r src/middleware dist/ && cp -r src/models dist/ && cp -r src/routes dist/ && cp -r src/services dist/ && cp -r src/utils dist/",
    "dev:hex": "cross-env NODE_ENV=development USE_HEXAGONAL=true ts-node --transpile-only src/index.ts",
    "start:ts": "cross-env NODE_ENV=production USE_HEXAGONAL=true ts-node src/index.ts",
    "dev:ts": "cross-env NODE_ENV=development USE_HEXAGONAL=true ts-node --transpile-only src/index.ts",
    "dev:ts:watch": "cross-env NODE_ENV=development USE_HEXAGONAL=true nodemon --exec ts-node --transpile-only src/index.ts",
    "dev:legacy": "cross-env NODE_ENV=development USE_HEXAGONAL=false node src/index.js",
    "start:legacy": "cross-env NODE_ENV=production USE_HEXAGONAL=false node src/index.js"
  },
  "description": "",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.20",
    "@api/systeme": "file:.api/apis/systeme",
    "@google-cloud/storage": "^7.7.0",
    "@types/cors": "^2.8.19",
    "@types/morgan": "^1.9.10",
    "@types/passport": "^1.0.17",
    "ai": "^4.3.10",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase-admin": "^12.7.0",
    "fuse.js": "^7.1.0",
    "gridfs-stream": "^1.1.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^6.4.3",
    "mongoose-paginate-v2": "^1.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^1.3.0",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "randomstring": "^1.3.0",
    "resend": "^4.3.0",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.10.0",
    "nodemon": "^3.1.9",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@10.9.0"
}
